row style=\"width: 950px;\" class=\"container\">\r\n            <el-button type=\"info\" plain @click=\"tranToLink(value.url)\" v-for=\"value in netlist2\" :key=\"value.id\">{{ value.name }}</el-button>\r\n        </el-row>\r\n\r\n    </div>\r\n\r\n    <br><br>\r\n\r\n    <div id=\"font1\"><el-divider content-position=\"left\"><span class=\"font2\">考试内容</span></el-divider></div>\r\n        <div id=\"button\">\r\n        <el-row style=\"width: 950px;\" class=\"container\">\r\n            <el-button type=\"info\" plain @click=\"tranToLink(value.url)\" v-for=\"value in netlist3\" :key=\"value.id\">{{ value.name }}</el-button>\r\n        </el-row>\r\n    </div>\r\n\r\n    <div>\r\n        <!-- 遍历分类对象 -->\r\n        <div v-for=\"(categoryList, categoryName) in categoryData\" :key=\"categoryName\">\r\n        <!-- 显示分类标题 -->\r\n        <div id=\"font1\">\r\n            <el-divider content-position=\"left\">\r\n            <span class=\"font2\">{{ categoryName }}</span>\r\n            </el-divider>\r\n        </div>\r\n        <!-- 显示分类下的按钮 -->\r\n        <div id=\"button\">\r\n            <el-row style=\"width: 950px;\" class=\"container\">\r\n            <el-button type=\"info\" plain @click=\"tranToLink(item.url)\" v-for=\"item in categoryList\" :key=\"item.id\">\r\n                {{ item.name }}\r\n            </el-button>\r\n            </el-row>\r\n        </div>\r\n        <br><br>\r\n        </div>\r\n    </div>\r\n\r\n</div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { Container } from 'element-ui';\r\nexport default {\r\n    data: function()\r\n    {\r\n        return{\r\n            temp:[],\r\n\r\n            netlist:[],\r\n            \r\n            netlist2:[],\r\n\r\n            netlist3:[],\r\n\r\n            categoryData: {\r\n                '学校内容': [],\r\n                '代码学习': [],\r\n                '考试内容': []\r\n            }\r\n        }\r\n    },\r\n\r\n    methods: {\r\n        tranToLink(url){\r\n         