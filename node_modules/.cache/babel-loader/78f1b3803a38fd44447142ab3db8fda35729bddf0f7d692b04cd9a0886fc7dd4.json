{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { Container } from 'element-ui';\nexport default {\n  data: function () {\n    return {\n      temp: [],\n      temp2: [],\n      tempcat: [],\n      netlist: [],\n      netlist2: [],\n      netlist3: [],\n      categoryData: {\n        \"1\": []\n      }\n    };\n  },\n  methods: {\n    tranToLink(url) {\n      location.href = url;\n    }\n  },\n  created: function () {\n    axios.get(\"http://localhost:8088/getSiteLearn\").then(response => {\n      this.temp = response.data;\n      for (let i = 0; i < this.temp.length; i++) {\n        switch (this.temp[i].type) {\n          case \"学校内容\":\n            this.netlist.push(this.temp[i]);\n            break;\n          case \"代码学习\":\n            this.netlist2.push(this.temp[i]);\n            break;\n          case \"考试内容\":\n            this.netlist3.push(this.temp[i]);\n            break;\n        }\n        console.log(this.temp[i].type);\n      }\n    });\n    axios.get(\"http://localhost:8088/selectCategory\").then(response => {\n      this.temp2 = response.data;\n      // console.log(response.data)\n      for (let i = 0; i <= this.temp2.length; i++) {\n        this.categoryData[this.temp2[i].name].push(this.temp2[i].name, this.temp[i].url);\n      }\n      console.log(this.categoryData);\n    });\n  }\n};","map":{"version":3,"names":["axios","Container","data","temp","temp2","tempcat","netlist","netlist2","netlist3","categoryData","methods","tranToLink","url","location","href","created","get","then","response","i","length","type","push","console","log","name"],"sources":["src/components/Learn.vue"],"sourcesContent":["<template>\r\n\r\n<div>    \r\n   \r\n\r\n    <div>\r\n        <!-- 遍历分类对象 -->\r\n        <div v-for=\"(categoryName, categoryList) in categoryData\" :key=\"categoryName\">\r\n        <!-- 显示分类标题 -->\r\n        <div id=\"font1\">\r\n            <el-divider content-position=\"left\">\r\n            <span class=\"font2\">{{ categoryName }}</span>\r\n            </el-divider>\r\n        </div>\r\n        <!-- 显示分类下的按钮 -->\r\n        <div id=\"button\">\r\n            <el-row style=\"width: 950px;\" class=\"container\">\r\n            <el-button type=\"info\" plain @click=\"tranToLink(item.url)\" v-for=\"item in categoryList\" :key=\"item.name\">\r\n                {{ item.name }}\r\n            </el-button>\r\n            </el-row>\r\n        </div>\r\n        <br><br>\r\n        </div>\r\n    </div>\r\n\r\n</div>\r\n\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { Container } from 'element-ui';\r\nexport default {\r\n    data: function()\r\n    {\r\n        return{\r\n            temp:[],\r\n\r\n            temp2:[],\r\n\r\n            tempcat:[],\r\n\r\n            netlist:[],\r\n            \r\n            netlist2:[],\r\n\r\n            netlist3:[],\r\n\r\n            categoryData: {\r\n                \"1\": []\r\n        }\r\n        }\r\n    },\r\n\r\n    methods: {\r\n        tranToLink(url){\r\n            location.href = url\r\n        }\r\n    },\r\n      created:function(){\r\n        axios.get(\"http://localhost:8088/getSiteLearn\").then((response)=>{\r\n            this.temp=response.data;\r\n            for(let i=0;i<this.temp.length;i++){\r\n\r\n                switch(this.temp[i].type){\r\n                    case \"学校内容\":\r\n                        this.netlist.push(this.temp[i])\r\n                        break;\r\n                    \r\n                    case \"代码学习\":\r\n                        this.netlist2.push(this.temp[i])\r\n                        break;\r\n\r\n                    case \"考试内容\":\r\n                        this.netlist3.push(this.temp[i])\r\n                        break;\r\n\r\n                }\r\n                console.log(this.temp[i].type)\r\n            }\r\n            \r\n\r\n        })\r\n\r\n        axios.get(\"http://localhost:8088/selectCategory\").then((response)=>{\r\n            this.temp2=response.data\r\n            // console.log(response.data)\r\n            for(let i=0;i<=this.temp2.length;i++){\r\n                this.categoryData[this.temp2[i].name].push(this.temp2[i].name, this.temp[i].url)\r\n                \r\n            }\r\n            console.log(this.categoryData)\r\n        })\r\n\r\n        \r\n\r\n}\r\n}\r\n</script>\r\n\r\n<style>\r\n    .font2{\r\n        text-align: left;\r\n        font-size: 15px;\r\n        margin-bottom: auto;\r\n    }\r\n\r\n    #button{\r\n      display: flex;\r\n      align-items: flex-start;\r\n    }\r\n\r\n    #button2{\r\n        display: flex;\r\n        align-items: flex-start;\r\n    }\r\n\r\n    hr{\r\n      border: 0; /* 清除默认样式 */\r\n      height: 1px; /* 分隔线高度 */\r\n      background-color: #ccc; /* 分隔线颜色 */\r\n      margin-top: 1px;\r\n      margin-bottom: 10px;\r\n    }\r\n\r\n    .container{\r\n        display:flex;\r\n        flex-wrap: wrap;\r\n        align-items: start;\r\n        gap: 10px;\r\n    }\r\n</style>"],"mappings":";AAiCA,OAAAA,KAAA;AACA,SAAAC,SAAA;AACA;EACAC,IAAA,WAAAA,CAAA,EACA;IACA;MACAC,IAAA;MAEAC,KAAA;MAEAC,OAAA;MAEAC,OAAA;MAEAC,QAAA;MAEAC,QAAA;MAEAC,YAAA;QACA;MACA;IACA;EACA;EAEAC,OAAA;IACAC,WAAAC,GAAA;MACAC,QAAA,CAAAC,IAAA,GAAAF,GAAA;IACA;EACA;EACAG,OAAA,WAAAA,CAAA;IACAf,KAAA,CAAAgB,GAAA,uCAAAC,IAAA,CAAAC,QAAA;MACA,KAAAf,IAAA,GAAAe,QAAA,CAAAhB,IAAA;MACA,SAAAiB,CAAA,MAAAA,CAAA,QAAAhB,IAAA,CAAAiB,MAAA,EAAAD,CAAA;QAEA,aAAAhB,IAAA,CAAAgB,CAAA,EAAAE,IAAA;UACA;YACA,KAAAf,OAAA,CAAAgB,IAAA,MAAAnB,IAAA,CAAAgB,CAAA;YACA;UAEA;YACA,KAAAZ,QAAA,CAAAe,IAAA,MAAAnB,IAAA,CAAAgB,CAAA;YACA;UAEA;YACA,KAAAX,QAAA,CAAAc,IAAA,MAAAnB,IAAA,CAAAgB,CAAA;YACA;QAEA;QACAI,OAAA,CAAAC,GAAA,MAAArB,IAAA,CAAAgB,CAAA,EAAAE,IAAA;MACA;IAGA;IAEArB,KAAA,CAAAgB,GAAA,yCAAAC,IAAA,CAAAC,QAAA;MACA,KAAAd,KAAA,GAAAc,QAAA,CAAAhB,IAAA;MACA;MACA,SAAAiB,CAAA,MAAAA,CAAA,SAAAf,KAAA,CAAAgB,MAAA,EAAAD,CAAA;QACA,KAAAV,YAAA,MAAAL,KAAA,CAAAe,CAAA,EAAAM,IAAA,EAAAH,IAAA,MAAAlB,KAAA,CAAAe,CAAA,EAAAM,IAAA,OAAAtB,IAAA,